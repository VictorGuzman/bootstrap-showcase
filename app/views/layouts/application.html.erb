<!DOCTYPE html>
<html>
<head>
  <title>BootstrapShowcase</title>
  <%= stylesheet_link_tag '//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css', media: 'all' %>
  <%= stylesheet_link_tag '//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css', media: 'all' %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag '//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js' %>
  <%= javascript_include_tag '//code.highcharts.com/highcharts.js' %>
  <%= javascript_include_tag '//ajax.aspnetcdn.com/ajax/jquery.validate/1.13.1/jquery.validate.min.js' %>
  <%= csrf_meta_tags %>
</head>
<body>
    <%= yield %>
</body>
</html>
